<package xmlns="http://expath.org/ns/pkg"
         name="http://fgeorges.org/test/invoice"
         abbrev="invoice"
         version="1.0"
         spec="1.0">

   <title>Invoice, a sample XProc package</title>

   <!--xquery>
      <import-uri>http://fgeorges.org/test/invoice-main.xq</import-uri>
      <file>invoice-main.xq</file>
   </xquery-->
   <xquery>
      <namespace>http://fgeorges.org/test/invoice.xql</namespace>
      <file>invoice.xql</file>
   </xquery>
   <xslt>
      <import-uri>http://fgeorges.org/test/invoice.xsl</import-uri>
      <file>invoice.xsl</file>
   </xslt>
   <rnc>
      <import-uri>http://fgeorges.org/test/invoice.rnc</import-uri>
      <file>invoice.rnc</file>
   </rnc>
   <rng>
      <import-uri>http://fgeorges.org/test/invoice.rng</import-uri>
      <file>invoice.rng</file>
   </rng>
   <xproc>
      <import-uri>http://fgeorges.org/test/invoice.xpl</import-uri>
      <file>invoice.xpl</file>
   </xproc>
   <schematron>
      <import-uri>http://fgeorges.org/test/invoice.sch</import-uri>
      <file>invoice.sch</file>
   </schematron>
   <schematron>
      <import-uri>http://fgeorges.org/test/invoice-fragment.sch</import-uri>
      <file>invoice-fragment.sch</file>
   </schematron>
   <xsd>
      <namespace>http://fgeorges.org/test/external-invoice</namespace>
      <file>invoice.xsd</file>
   </xsd>

</package>
